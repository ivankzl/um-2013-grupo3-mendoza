<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="canario" match-name="mensaje"/>
  <table-filter match-catalog="canario" match-name="tema"/>
  <table-filter match-catalog="canario" match-name="temaMensaje"/>
  <table-filter match-catalog="canario" match-name="tipoMensaje"/>
  <table-filter match-catalog="canario" match-name="tipoUsuario"/>
  <table-filter match-catalog="canario" match-name="usuario"/>
  <table-filter match-catalog="canario" match-name="usuariosMencionadosMensaje"/>
</hibernate-reverse-engineering>	